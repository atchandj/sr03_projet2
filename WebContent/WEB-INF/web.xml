<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Projet 2 en SR03</display-name>
  <description>Evaluation de stagiaire</description>
  <welcome-file-list>
    <welcome-file>connection</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
    </jsp-property-group>
  </jsp-config>
  <servlet>
    <servlet-name>Deconnection</servlet-name>
    <servlet-class>servlets.Deconnection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Deconnection</servlet-name>
    <url-pattern>/deconnection</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Connection</servlet-name>
    <servlet-class>servlets.Connection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Connection</servlet-name>
    <url-pattern>/connection</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TraineeMenu</servlet-name>
    <servlet-class>servlets.TraineeMenu</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TraineeMenu</servlet-name>
    <url-pattern>/trainee</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SuperUserMenu</servlet-name>
    <servlet-class>servlets.SuperUserMenu</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SuperUserMenu</servlet-name>
    <url-pattern>/super_user</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>TraineeFilter</filter-name>
    <filter-class>filters.TraineeFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>TraineeFilter</filter-name>
    <url-pattern>/trainee/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>SuperUserFilter</filter-name>
    <filter-class>filters.SuperUserFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SuperUserFilter</filter-name>
    <url-pattern>/super_user/*</url-pattern>
  </filter-mapping>
</web-app>